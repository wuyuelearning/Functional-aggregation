<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="text_describe">d
        <!-- Customize your theme here. -->
        <item name="android:layout_width">140dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">#FD3C71</item>

    </style>

    <style name="AnimationPreview">
        <item name="android:windowEnterAnimation">@anim/popupwindow_popup</item>
        <item name="android:windowExitAnimation">@anim/popupwindow_closed</item>
    </style>

    <style name="AnimationPreview2">
        <item name="android:windowEnterAnimation">@anim/slide_in_from_top</item>
        <item name="android:windowExitAnimation">@anim/slide_out_to_top</item>
    </style>

    <style name="style_16_tab_selector">
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">@color/color_666666_to_d30775_selector</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

</resources>
